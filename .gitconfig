# user
[user]
	email = mail@mail.com
	name = username

# aliases
[alias]
	l = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all -14
	lo = log --oneline -10
	log = log --pretty=oneline -n 20 --graph --abbrev-commit
	ss = status -s
	s = status
	a = add
	aa = add .
	c = commit -m
	ra = remote add
	r = remote
	rv = remote -v
	b = branch
	cout = checkout
	sw = switch
	aliases = config --get-regexp alias
	whoami = config user.email
